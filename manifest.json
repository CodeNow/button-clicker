{
  "manifest_version": 2,
  "name": "Runnable Button Clicker",
  "description": "Scripts to automate instance management",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>", "http://*/*"],
    "js": ["loader.js"],
    "all_frames": true,
    "run_at": "document_end",
    "match_about_blank": false
  }]
}

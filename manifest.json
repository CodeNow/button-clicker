{
  "manifest_version": 2,
  "name": "Runnable Button Clicker",
  "description": "Scripts to automate instance management",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [{
    "matches": [
      "*://*.runnable.com*",
      "*://*.runnable.io*",
      "*://*.runnableapp.com*",
      "*://*.runnable-staging.com*",

      "*://*.runnable-gamma.com*",
      "*://*.runnable.ninja*",

      "*://*.runnable-beta.com*",
      "*://*.runnablecloud.com*"
    ],
    "js": ["loader.js"],
    "all_frames": true,
    "run_at": "document_end",
    "match_about_blank": false
  }]
}
